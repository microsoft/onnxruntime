{
    "build_osx_archs": {
        "iphonesimulator": [
            "x86_64"
        ]
    },
    "build_params": [
        "--ios",
        "--parallel",
        "--use_xcode",
        "--build_apple_framework",
        "--minimal_build=extended",
        "--disable_rtti",
        "--disable_ml_ops",
        "--disable_exceptions",
        "--enable_reduced_operator_type_support",
        "--use_coreml",
        "--skip_tests",
        "--apple_deploy_target=12.0"
    ]
}

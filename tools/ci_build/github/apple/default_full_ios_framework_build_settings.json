{
    "build_osx_archs": {
        "iphoneos": [
            "arm64"
        ],
        "iphonesimulator": [
            "arm64",
            "x86_64"
        ],
        "macabi": [
            "arm64",
            "x86_64"
        ]
    },
    "build_params": {
        "base": [
            "--parallel",
            "--build_apple_framework",
            "--use_coreml",
            "--skip_tests",
            "--cmake_extra_defines=onnxruntime_BUILD_UNIT_TESTS=OFF"
        ],
        "iphoneos": [
            "--ios",
            "--use_xcode",
            "--use_xnnpack",
            "--apple_deploy_target=13.0"
        ],
        "iphonesimulator": [
            "--ios",
            "--use_xcode",
            "--use_xnnpack",
            "--apple_deploy_target=13.0"
        ],
        "macabi":[
            "--macos=Catalyst",
            "--apple_deploy_target=14.0"
        ]
    }
}

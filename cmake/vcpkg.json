{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "onnxruntime",
  "version-date": "2025-01-31",
  "description": "ONNX Runtime: cross-platform, high performance ML inferencing and training accelerator",
  "homepage": "https://onnxruntime.ai/",
  "license": "MIT",
  "dependencies": [
    "abseil",
    {
      "name": "benchmark",
      "platform": "!android"
    },
    {
      "name": "boost-config",
      "version>=": "1.82.0"
    },
    {
      "name": "boost-mp11",
      "version>=": "1.82.0"
    },
    {
      "name": "cpuinfo",
      "platform": "!osx"
    },
    "cxxopts",
    "date",
    "dlpack",
    "eigen3",
    {
      "name": "flatbuffers",
      "host": true,
      "version>=": "23.5.26"
    },
    {
      "name": "flatbuffers",
      "version>=": "23.5.26"
    },
    {
      "name": "mimalloc",
      "platform": "windows"
    },
    "ms-gsl",
    "nlohmann-json",
    "onnx",
    "optional-lite",
    {
      "name": "protobuf",
      "version>=": "3.21.12"
    },
    {
      "name": "protobuf",
      "host": true,
      "version>=": "3.21.12"
    },
    "pybind11",
    "re2",
    "safeint",
    "utf8-range",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "wil",
      "platform": "windows"
    },
    {
      "name": "zlib",
      "platform": "windows"
    }
  ],
  "features": {
    "tests": {
      "description": "Build ONNXRuntime unit tests",
      "dependencies": [
        "gtest"
      ]
    },
    "xnnpack-ep": {
      "description": "Build with XNNPack EP",
      "dependencies": [
        "xnnpack"
      ]
    },
    "coreml-ep": {
      "description": "Build with CoreML EP",
      "dependencies": [
        "fp16"
      ]
    },
    "dml-ep": {
      "description": "Build with CoreML EP",
      "dependencies": [
        "directx-headers"
      ]
    }
  },
  "overrides": [
    {
      "name": "protobuf",
      "version": "3.21.12#4"
    },
    {
      "name": "flatbuffers",
      "version": "23.5.26"
    }
  ]
}
